{
  "name": "dom-gen",
  "version": "1.2.1",
  "description": "Utility for dom generation, a jquery plugin",
  "main": "dist.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "dist": "babel src.js > dist.js",
    "tags": "awk '/^export const [a-z0-9]+ = domGen/{print $3\"()\"}' < src.js",
    "prepublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt3k/dom-gen.git"
  },
  "keywords": [
    "dom",
    "jqueryplugin"
  ],
  "author": "Yoshiya Hinosawa <stibium121@gmail.com> (https://twitter.com/kt3k)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kt3k/dom-gen/issues"
  },
  "homepage": "https://github.com/kt3k/dom-gen#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "jquery": "^2.2.3",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-chrome-launcher": "^0.2.3",
    "karma-mocha": "^0.2.2",
    "mocha": "^2.4.5",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transforms": [
      ["babelify"]
    ]
  }
}
